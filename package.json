{
  "author": "Jules Bonnard @julesbonnard",
  "bin": {
    "apicore": "./bin/run.js"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.2.0",
    "@oclif/core": "^4.1.0",
    "@types/fs-extra": "^11.0.4",
    "afpnews-api": "^2.1.3",
    "chalk": "^5.3.0",
    "lodash": "^4.17.21",
    "natural-orderby": "^5.0.0",
    "ora": "^8.1.1",
    "string-width": "^7.2.0",
    "zod": "^3.24.1"
  },
  "description": "A modern CLI to get fresh news from apicore",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/chai": "^5",
    "@types/lodash": "^4.17.13",
    "@types/mocha": "^10",
    "@types/node": "^22",
    "eslint": "^9",
    "eslint-plugin-n": "^17.15.0",
    "mocha": "^11",
    "oclif": "^4.17.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/julesbonnard/apicore-cli",
  "license": "MIT",
  "main": "",
  "name": "@julesbonnard/apicore-cli",
  "oclif": {
    "bin": "apicore",
    "dirname": "apicore",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "julesbonnard/apicore-cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "version": "0.2.0",
  "bugs": "https://github.com/julesbonnard/apicore-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "type": "module"
}
